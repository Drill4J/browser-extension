{
  "name": "@drill4j/browser-extension",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint \"./**/*.jsx\" \"./**/*.js\"",
    "format:js": "npm run lint -- --fix",
    "format": "npm run format:js && prettier --write */**/*.scss */**/*.md",
    "dev": "webpack-dev-server --mode development --inline=false --port 3001",
    "build": "webpack -p --env.production",
    "test": "jest --notify",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:lang:en": "webpack --display none --env.dev && babel localization/translator.js | node",
    "manage:translations": "webpack --display none --env.dev && babel-node ./localization/translatorRunner.js",
    "manage:translations:test": "webpack --display none --env.dev && babel-node ./localization/translatorRunnerCheck.js",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
  },
  "dependencies": {
    "@redneckz/react-bem-helper": "^2.0.3",
    "axios": "^0.18.0",
    "classnames": "2.2.6",
    "cookie": "^0.3.1",
    "dotenv": "5.0.1",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "html-react-parser": "0.4.7",
    "intl": "1.2.5",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "react": "16.8.6",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.8.6",
    "react-draggable": "3.1.1",
    "react-intl": "2.7.2",
    "react-resizable": "^1.7.5",
    "webextension-polyfill": "0.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.4",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-airbnb": "2.4.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.11",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "0.28.11",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "enzyme-to-json": "3.3.3",
    "eslint": "5.11.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "^4.11.0",
    "postcss-loader": "2.1.5",
    "prettier": "1.14.3",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "4.3.11",
    "react-intl-translations-manager": "5.0.3",
    "redux-devtools-extension": "2.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "svg-inline-loader": "0.8.0",
    "url-loader": "1.1.2",
    "webpack": "4.28.2",
    "webpack-chrome-extension-reloader": "1.1.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14",
    "webpack-hot-middleware": "2.24.3",
    "webpack-notifier": "1.7.0",
    "write-file-webpack-plugin": "4.5.0"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^components[/](.+)": "<rootDir>/src/components/$1",
      "^controllers[/](.+)": "<rootDir>/src/controllers/$1",
      "^common[/](.+)": "<rootDir>/src/common/$1",
      "^pages[/](.+)": "<rootDir>/src/pages/$1",
      "^store[/](.+)": "<rootDir>/src/store/$1",
      "^routes[/](.+)": "<rootDir>/src/routes/$1",
      "^layouts[/](.+)": "<rootDir>/src/layouts/$1"
    },
    "notify": false
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
