{
  "name": "Drill4J Browser Extension",
  "version": "0.0.1",
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "storage",
    "http://localhost/*",
    "http://*.epam.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "default-logo.png",
      "128": "large-logo.png"
    },
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
